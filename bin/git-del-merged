#!/usr/bin/env oil

git branch --merged | while read --line {
  var line = $_line.strip()
  if (line !== 'main' and line !== 'develop' and line !== 'master' and not line.startswith('*')) {
    echo $line
  }
} | readarray -t :branches

if (len(branches) === 0) {
  echo "No merged branches."
} else {
  git branch -D @branches
}
