root = true

[*]
charset = utf-8
end_of_line = lf
trim_trailing_whitespace = true
insert_final_newline = true

indent_style = space
indent_size = 2

[*Makefile]
indent_style = tab

[*.go]
indent_size = 4
indent_style = tab

[*.go.y]
indent_style = tab

[*.{c,h}]
indent_style = tab

[*.py]
indent_size = 4

[*.{cpp,hpp}]
indent_size = 4

[*.rs]
indent_size = 4

[*.d]
indent_size = 4

[*.{js,jsx,ts,tsx}]
indent_size = 2

[*.{kt,java}]
indent_size = 4

[*.proto]
indent_sie = 2
